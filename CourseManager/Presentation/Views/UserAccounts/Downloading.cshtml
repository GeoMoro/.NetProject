@model FileViewModel
@{
    ViewData["Title"] = "Downloading";
    Layout = "_Layout";
}

<h2>Downloading</h2>
<div class="row">
    <div class="col-md-4">
        <form runat="server" enctype="multipart/form-data" asp-controller="UserAccounts" asp-action="Download">
            @*<div asp-validation-summary="ModelOnly" class="text-danger"></div>*@
            <button type="submit">Button 1</button>
            
            <br /><br />

            <asp:Label runat="server" id="StatusLabel" text="Download status: " />
        </form>
    </div>
</div>

